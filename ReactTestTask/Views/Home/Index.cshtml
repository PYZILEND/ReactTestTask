@model HomeViewModel
@{
    ViewData["Title"] = "Home Page";
}

@Html.React("UserList", new
{
    data = Model.Users,
    getUrl = "Users",
    postUrl = "Users/Edit",
    pollInterval = 2000,
})
<br/>
@Html.React("Metrics", new
{
    retention = Model.RollingRetention,
    distribution = Model.LifetiemeDistribution,
    getMetricsUrl = "Metrics"
})
